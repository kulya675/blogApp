ct-apps/blog-app-v2/src/components/App/App.tsx",[],"/home/kirill/react-apps/blog-app-v2/src/containers/Header/Header.tsx",["179"],"/* eslint-disable jsx-a11y/no-static-element-interactions */\n/* eslint-disable jsx-a11y/click-events-have-key-events */\nimport React, { useState, useEffect } from 'react';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { useCookies } from 'react-cookie';\nimport { Link, useHistory } from 'react-router-dom';\n\nimport { Button, Avatar, makeStyles } from '@material-ui/core';\nimport { DropdownMenu } from '../../components/DropdownMenu/DropdownMenu';\n\nimport { userSelector } from '../../redux/selectors';\nimport { logOut } from '../../redux/actions/userActions';\nimport { getCurrentUser } from '../../redux/thunkActions/userThunkAction';\n\nimport type { RootStateType } from '../../redux/rootReducer';\n\nimport avatar from './avatar.svg';\nimport styles from './Header.module.scss';\n\nconst useStyles = makeStyles({\n  button: {\n    borderRadius: 3,\n    fontSize: 16,\n    textTransform: 'none',\n    textDecoration: 'none',\n  },\n  avatar: {\n    marginLeft: 13,\n    width: 46,\n    height: 46,\n  },\n});\n\nexport const Header: React.FC = () => {\n  const { isLoggedIn, user } = useSelector(userSelector);\n  const dispatch = useDispatch();\n  const [anchorEl, setAnchorEl] = useState<null | HTMLElement>(null);\n  const [cookie, , removeCookie] = useCookies(['token']);\n  const history = useHistory();\n  const classes = useStyles();\n\n  useEffect(() => {\n    if (cookie.token) {\n      dispatch(getCurrentUser(cookie.token));\n    }\n  }, [cookie.token, dispatch]);\n\n  const openDropdownHandler = (event: React.MouseEvent<HTMLElement>) => {\n    setAnchorEl(event.currentTarget);\n  };\n\n  const closeDropdownHandler = () => {\n    setAnchorEl(null);\n  };\n\n  const logOutHandler = () => {\n    removeCookie('token', { path: '/' });\n    dispatch(logOut());\n    history.push('/');\n  };\n\n  const loggedInRightSide = (\n    <>\n      <ul className={styles.list}>\n        <li className={styles.item}>\n          <Button className={classes.button} size=\"small\" color=\"secondary\" variant=\"outlined\">\n            <Link to=\"/new-article\">Create article</Link>\n          </Button>\n        </li>\n        <li className={styles.item}>\n          <div onClick={openDropdownHandler}>\n            <span className={styles.username}>{user.username}</span>\n            <Avatar className={classes.avatar} src={avatar} alt=\"avatar\" />\n          </div>\n          <DropdownMenu\n            anchorEl={anchorEl}\n            onClose={closeDropdownHandler}\n            closeHandler={closeDropdownHandler}\n            open={Boolean(anchorEl)}\n            onLogout={logOutHandler}\n          />\n        </li>\n      </ul>\n    </>\n  );\n\n  const loggedOutRightSide = (\n    <>\n      <ul className={styles.list}>\n        <li className={styles.item}>\n          <Button className={classes.button} size=\"large\">\n            <Link to=\"/sign-in\">Sign In</Link>\n          </Button>\n        </li>\n        <li className={styles.item}>\n          <Button className={classes.button} size=\"large\" color=\"primary\" variant=\"outlined\">\n            <Link to=\"/sign-up\">Sign Up</Link>\n          </Button>\n        </li>\n      </ul>\n    </>\n  );\n\n  const rightSide = isLoggedIn ? loggedInRightSide : loggedOutRightSide;\n\n  return (\n    <header className={styles.header}>\n      <div className={styles.wrapper}>\n        <h1>\n          <Link className={styles.link} to=\"/\">\n            Blog\n          </Link>\n        </h1>\n        {rightSide}\n      </div>\n    </header>\n  );\n};\n","/home/kirill/react-apps/blog-app-v2/src/redux/store.ts",[],"/home/kirill/react-apps/blog-app-v2/src/redux/rootReducer.ts",[],"/home/kirill/react-apps/blog-app-v2/src/redux/reducers/userReducer.ts",[],"/home/kirill/react-apps/blog-app-v2/src/redux/actions/userActions.ts",[],"/home/kirill/react-apps/blog-app-v2/src/r